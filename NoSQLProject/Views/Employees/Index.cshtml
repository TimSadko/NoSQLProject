@model List<Employee>

<h1>Employees</h1>

<table>
	<thead>
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Email</th>
			<th>Role</th>
		</tr>
	</thead>
	<tbody>
		@{
			foreach(Employee employee in Model)
			{
				<tr>
					<th>@employee.FirstName</th>
					<th>@employee.LastName</th>
					<th>@employee.Email</th>
					<th>@employee.Role</th>
				</tr>
			}
		}
	</tbody>
</table>
